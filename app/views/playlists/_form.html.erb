<%= form_for(@playlist) do |f| %>
  <%= f.text_field :name %>
  <% Song.all.each do |song| %>
  <div>
      <%= check_box_tag(:song_ids, song.id, @playlist_songs.include?(song.id), name: 'playlist[song_ids][]', id:"song-#{song.id}") %>
      <%= song.title %>
    <% end %>
  </div>
  <%= f.submit %>
<% end %>
